<body>
<div class="container">
  <h1>Generate Your Key</h1>
  <p id="countdown">Generating key in <span id="timer">5</span> seconds...</p>
  <div id="key"></div>
  <button id="copyBtn" style="display: none;">Copy Key</button>
</div>

<script>
function generateKey() {
  // Generate a random string (for demonstration purposes, generating a random string of length 10)
  var key = '';
  var characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
  var charactersLength = characters.length;
  for (var i = 0; i < 10; i++) {
    key += characters.charAt(Math.floor(Math.random() * charactersLength));
  }
  return key;
}

function startTimer(duration, display) {
  var timer = duration;
  var countdown = setInterval(function () {
    display.textContent = timer;
    if (--timer < 0) {
      clearInterval(countdown);
      var key = '';
      var hwid = getHWID();
      var ip = getIP();
      var storedKey = localStorage.getItem('key_' + hwid) || localStorage.getItem('key_' + ip);
      if (storedKey) {
        key = storedKey;
      } else {
        key = generateKey();
        localStorage.setItem('key_' + hwid, key);
        localStorage.setItem('key_' + ip, key);
        setTimeout(function() {
          localStorage.removeItem('key_' + hwid);
          localStorage.removeItem('key_' + ip);
        }, 24 * 60 * 60 * 1000); // Remove the key after 24 hours
      }
      document.getElementById('key').textContent = key;
      document.getElementById('copyBtn').style.display = 'inline-block';
    }
  }, 1000);
}

function getHWID() {
  // For demonstration purposes, generating a random HWID
  return Math.random().toString(36).substring(2, 15) + Math.random().toString(36).substring(2, 15);
}

function getIP() {
  // For demonstration purposes, generating a random IP
  return Math.floor(Math.random() * 256) + '.' +
         Math.floor(Math.random() * 256) + '.' +
         Math.floor(Math.random() * 256) + '.' +
         Math.floor(Math.random() * 256);
}

document.addEventListener("DOMContentLoaded", function() {
  var fiveSeconds = 5,
      display = document.querySelector('#timer');
  startTimer(fiveSeconds, display);

  document.getElementById('copyBtn').addEventListener('click', function() {
    var keyText = document.getElementById('key').textContent;
    navigator.clipboard.writeText(keyText).then(function() {
      alert('Key copied to clipboard: ' + keyText);
    }, function(err) {
      console.error('Could not copy text: ', err);
    });
  });
});
</script>
</body>
</html>
